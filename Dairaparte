<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <!-- Definición de tipos complejos -->

    <!-- Gestión de Pacientes -->
    <xs:element name="RegistrarPaciente">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Nombre" type="xs:string"/>
                <xs:element name="Apellido" type="xs:string"/>
                <xs:element name="FechaNacimiento" type="xs:date"/>
                <xs:element name="Telefono" type="xs:string"/>
                <xs:element name="CorreoElectronico" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="RegistrarPacienteResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="ID_Paciente" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="ActualizarPacienteRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="ID_Paciente" type="xs:string"/>
                <xs:element name="Nombre" type="xs:string" minOccurs="0"/>
                <xs:element name="Apellido" type="xs:string" minOccurs="0"/>
                <xs:element name="FechaNacimiento" type="xs:date" minOccurs="0"/>
                <xs:element name="Telefono" type="xs:string" minOccurs="0"/>
                <xs:element name="CorreoElectronico" type="xs:string" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="ConsultarPacienteRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="ID_Paciente" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="ConsultarPaciente">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Nombre" type="xs:string"/>
                <xs:element name="Apellido" type="xs:string"/>
                <xs:element name="FechaNacimiento" type="xs:date"/>
                <xs:element name="Telefono" type="xs:string"/>
                <xs:element name="CorreoElectronico" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Gestión de Especialidades -->
    <xs:element name="RegistrarEspecialidadRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Medicina_General" type="xs:string"/>
                 <xs:element name="Pediatria" type="xs:string"/>
                 <xs:element name="Dermatologia" type="xs:string"/>
